{{#trans.searchBtnTitle}}
	<label id="xrx_bnrv4_menu_search_default_btn" for="xrx_bnrv4_header_defaultchk" title="{{trans.searchBtnTitle}}" class="xrx_bnrv4_header_search_btn">
		{{> mustachePartials/search_icon }}
	</label>
	<label id="xrx_bnrv4_menu_search_btn" for="xrx_bnrv4_header_searchchk" title="{{trans.searchBtnTitle}}" class="xrx_bnrv4_header_search_btn">
		{{> mustachePartials/search_icon }}
	</label>
	<div id="xrx_bnrv4_header_search">
		<form method="get" action="{{trans.searchURL}}" id="xrx_bnr_hdr_utilitynav_search_form" novalidate role="search">
			<button type="submit"></button>
			<div class="xrx_bnrv4_header_search_submit_icon">
				{{> mustachePartials/search_icon }}
			</div>
			<input type="textbox" title="Search" placeholder="{{trans.searchPlaceholder}}" name="searchString"></input>
			<input name="XOGlang" value="{{XOGLang}}" type="hidden" />
		</form>
	</div>
{{/trans.searchBtnTitle}}
