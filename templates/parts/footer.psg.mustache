<div id="xrx_bnrv4_footer">
	{{#footer.lobFooter}}
		<div id="xrx_bnrv4_lobfooter" class="xrx_bnrv4_footer_position">
			<div class="xrx_bnrv4_table">
				<div class="xrx_bnrv4_row">
					<div class="xrx_bnrv4_column">
						<div class="xrx_bnrv4_lobfooter_title">{{trans.usefulLinks}}</div>

						<div class="xrx_bnrv4_table">
							<div class="xrx_bnrv4_row">
								<div class="xrx_bnrv4_column">
									{{#psgSales}}
										<div id="xrx_bnrv4_lobfooter_sales">
											{{> column_header }}
											{{> list}}
										</div>
									{{/psgSales}}
								</div>
								<div class="xrx_bnrv4_column">
									{{#psgProducts}}
										<div id="xrx_bnrv4_lobfooter_products">
											{{> column_header}}
											{{> list}}
										</div>
									{{/psgProducts}}
									{{#psgOwners}}
										<div id="xrx_bnrv4_lobfooter_owners">
											{{> column_header}}
											{{> list}}
										</div>
									{{/psgOwners}}
								</div>
								<div class="xrx_bnrv4_column">
									{{#psgHelpfulResources}}
										<div id="xrx_bnrv4_lobfooter_resources">
											{{> column_header}}
											{{> list}}
										</div>
									{{/psgHelpfulResources}}
								</div>
								<div class="xrx_bnrv4_column">
									{{#psgApplications}}
										<div id="xrx_bnrv4_lobfooter_applications">
											{{> column_header}}
											{{> list}}
										</div>
									{{/psgApplications}}
								</div>
							</div>
						</div>

					</div>
					<div class="xrx_bnrv4_column">
						<div class="xrx_bnrv4_lobfooter_title">{{trans.connect}}</div>

						{{> ../includes/connect}}
					</div>
				</div>
			</div>
		</div>
	{{/footer.lobFooter}}

	{{> mustachePartials/footer_main }}
</div>

{{#dataStr}}
	<script type='text/javascript'>
		var xrx_bnrv4_vars = {{{dataStr}}};
	</script>
{{/dataStr}}

