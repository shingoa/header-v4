{{#with header}}
	<div id="xrx_bnrv4_header" class="xrx_bnr_handle_jump_link">
		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_defaultchk" id="xrx_bnrv4_header_defaultchk" checked="checked" autocomplete="off" aria-hidden="true" />

		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_menuchk" id="xrx_bnrv4_header_menuchk" autocomplete="off" aria-hidden="true" />
		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_searchchk" id="xrx_bnrv4_header_searchchk" autocomplete="off" aria-hidden="true" />

		<div id="xrx_bnrv4_header_floater">
			<div>
				<div class="xrx_bnrv4_table">
					<div>
						<div id="xrx_bnrv4_header_logocol">
							<a class="xrx_bnrv4_logo" name="&lid=hdr-pre-logo-home" href="{{trans.logoURL}}" title="{{trans.logoTitle}}"></a>
							<div id="xrx_bnr_v4_floating_title"></div>
						</div>

						<div id="xrx_bnrv4_header_searchcol">
							{{> includes/search }}
							<div id="xrx_bnr_v4_floating_content"></div>
						</div>

						<div id="xrx_bnrv4_header_secondarycol">
							{{#with secondaryNav}}
								<ul id="xrx_bnrv4_header_nav_secondary">
									{{#each links}}
										<li id="{{id}}">
											{{> includes/partials/secondary_nav_link}}
										</li>
									{{/each}}
								</ul>
							{{/with}}
						</div>

						<div id="xrx_bnrv4_header_menucol">
							<label
								for="xrx_bnrv4_header_defaultchk"
								name="&amp;lid=hdr-pre-int-menu-close"
								id="xrx_bnrv4_menu_defaultbtn"
								title="{{trans.menuBtnTitle}}"
								class="xrx_bnrv4_menu_btn"
								aria-label="{{trans.menuBtnTitle}}"
								role="button"
								aria-controls="xrx_bnrv4_header_nav">
								<span>{{trans.menuLabel}}</span>
								{{> parts/partials/menu_icon }}
							</label>

							<label
								for="xrx_bnrv4_header_menuchk"
								name="&amp;lid=hdr-pre-int-menu"
								id="xrx_bnrv4_menu_openbtn"
								title="{{trans.menuBtnTitle}}"
								class="xrx_bnrv4_menu_btn"
								aria-label="{{trans.menuBtnTitle}}"
								role="button"
								aria-controls="xrx_bnrv4_header_nav">
								<span>{{trans.menuLabel}}</span>
								{{> parts/partials/menu_icon }}
							</label>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div id="xrx_bnrv4_header_nav" class="xrx_bnrv4_static_header">
			<input type="radio" name="xrx_bnrv4_header_nav_primary" value="xrx_bnrv4_default" id="xrx_bnrv4_default" checked="checked" autocomplete="off" />

			<div>
				<a class="xrx_bnrv4_logo" name="&lid=hdr-pre-logo-home" href="{{trans.logoURL}}" title="{{trans.logoTitle}}"></a>

				{{#with primaryNav}}
					<ul id="xrx_bnrv4_header_nav_primary" role="tablist">
						{{> includes/services}}
						{{> includes/products}}
						{{> includes/insights}}
						{{> includes/supplies}}
						{{> includes/support}}
						{{> includes/about}}
					</ul>
				{{/with}}

				{{#with tertiaryNav}}
					<ul id="xrx_bnrv4_header_nav_tertiary">
						<li id="xrx_bnrv4_header_nav_tertiary_search">
							<label for="searchString">
								{{> parts/partials/search_icon }}
							</label>
						</li>
						{{> includes/share_desktop}}
						{{> includes/contact_desktop}}
					</ul>
				{{/with}}

				{{#with secondaryNav}}
					<ul id="xrx_bnrv4_header_nav_secondary_mobile">
						{{#each links}}
							<li id="{{id}}_mobile">
								{{> includes/partials/secondary_nav_link}}
							</li>
						{{/each}}
					</ul>
				{{/with}}
			</div>
		</div>
	</div>
{{/with}}
