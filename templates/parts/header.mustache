{{#header}}
	<div id="xrx_bnrv4_header">
		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_defaultchk" id="xrx_bnrv4_header_defaultchk" checked />

		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_menuchk" id="xrx_bnrv4_header_menuchk" />
		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_searchchk" id="xrx_bnrv4_header_searchchk" />


		<div id="xrx_bnrv4_header_floater">
			<div>
				<label for="xrx_bnrv4_header_defaultchk" id="xrx_bnrv4_menu_defaultbtn" title="{{trans.menuBtnTitle}}" class="xrx_bnrv4_menu_btn">
					<span></span>
					<span></span>
					<span></span>
					<span></span>
				</label>
				<label for="xrx_bnrv4_header_menuchk" id="xrx_bnrv4_menu_openbtn" title="{{trans.menuBtnTitle}}" class="xrx_bnrv4_menu_btn">
					<span></span>
					<span></span>
					<span></span>
					<span></span>
				</label>
				<a class="xrx_bnrv4_logo" href="#" title="{{logoTitle}}"></a>

				<label id="xrx_bnrv4_menu_search_default_btn" for="xrx_bnrv4_header_defaultchk" title="{{trans.searchBtnTitle}}" class="xrx_bnrv4_header_search_btn">
					<svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
						 viewBox="0 0 18.7 19.9" style="enable-background:new 0 0 18.7 19.9;" xml:space="preserve">
						<style type="text/css">
							.st0{fill:none;stroke-width:2;stroke-miterlimit:10;}
							.st1{fill:none;stroke-width:2;stroke-linejoin:round;stroke-miterlimit:10;}
						</style>
						<circle id="XMLID_1_" class="st0" cx="7.9" cy="7.9" r="6.9" stroke="#999" />
						<line id="XMLID_3_" class="st1" x1="12.7" y1="13" x2="18" y2="19.2" stroke="#999" />
					</svg>
				</label>
				<label id="xrx_bnrv4_menu_search_btn" for="xrx_bnrv4_header_searchchk" title="{{trans.searchBtnTitle}}" class="xrx_bnrv4_header_search_btn">
					<svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
						 viewBox="0 0 18.7 19.9" style="enable-background:new 0 0 18.7 19.9;" xml:space="preserve">
						<style type="text/css">
							.st0{fill:none;stroke-width:2;stroke-miterlimit:10;}
							.st1{fill:none;stroke-width:2;stroke-linejoin:round;stroke-miterlimit:10;}
						</style>
						<circle id="XMLID_1_" class="st0" cx="7.9" cy="7.9" r="6.9" stroke="#999" />
						<line id="XMLID_3_" class="st1" x1="12.7" y1="13" x2="18" y2="19.2" stroke="#999" />
					</svg>
				</label>
				<div id="xrx_bnrv4_header_search">
					<input type="textbox" title="Search" placeholder="{{trans.searchPlaceholder}}"></input>
				</div>

				{{#header.tertiaryNav}}
					<ul id="xrx_bnrv4_header_nav_tertiary">
						@@include('../includes/contact.mustache')
						@@include('../includes/share.mustache')
					</ul>
				{{/header.tertiaryNav}}
			</div>
		</div>

		<div id="xrx_bnrv4_header_nav">
			<input type="radio" name="xrx_bnrv4_header_nav_primary" value="xrx_bnrv4_default" id="xrx_bnrv4_default" checked />

			<div>
				{{#header.primaryNav}}
					<ul id="xrx_bnrv4_header_nav_primary">
						@@include('../includes/services.mustache')
						@@include('../includes/products.mustache')
						@@include('../includes/insights.mustache')
						@@include('../includes/supplies.mustache')
						@@include('../includes/support.mustache')
						@@include('../includes/about.mustache')
					</ul>
				{{/header.primaryNav}}

				{{#header.secondaryNav}}
					<ul id="xrx_bnrv4_header_nav_secondary">
						{{#links}}
							{{> list_item}}
						{{/links}}
					</ul>
				{{/header.secondaryNav}}
			</div>
		</div>
	</div>
{{/header}}
