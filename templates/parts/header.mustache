{{#header}}
	<div id="xrx_bnrv4_header">
		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_defaultchk" id="xrx_bnrv4_header_defaultchk" checked="checked" autocomplete="off" />

		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_menuchk" id="xrx_bnrv4_header_menuchk" autocomplete="off" />
		<input type="radio" name="xrx_bnrv4_header_menu" value="xrx_bnrv4_header_searchchk" id="xrx_bnrv4_header_searchchk" autocomplete="off" />

		<div id="xrx_bnrv4_header_floater">
			<div>
				<label for="xrx_bnrv4_header_defaultchk" name="&amp;lid=hdr-pre-int-menu-close" id="xrx_bnrv4_menu_defaultbtn" title="{{trans.menuBtnTitle}}" class="xrx_bnrv4_menu_btn">
					{{> mustachePartials/menu_icon }}
				</label>
				<label for="xrx_bnrv4_header_menuchk" name="&amp;lid=hdr-pre-int-menu" id="xrx_bnrv4_menu_openbtn" title="{{trans.menuBtnTitle}}" class="xrx_bnrv4_menu_btn">
					{{> mustachePartials/menu_icon }}
				</label>
				<a class="xrx_bnrv4_logo" name="&lid=hdr-pre-logo-home" href="{{trans.logoURL}}" title="{{trans.logoTitle}}"></a>

				{{> ../includes/search }}

				{{#header.tertiaryNav}}
					<ul id="xrx_bnrv4_header_nav_tertiary">
						{{> ../includes/contact}}
						{{> ../includes/share}}
					</ul>
				{{/header.tertiaryNav}}
			</div>
		</div>

		<div id="xrx_bnrv4_header_nav">
			<input type="radio" name="xrx_bnrv4_header_nav_primary" value="xrx_bnrv4_default" id="xrx_bnrv4_default" checked="checked" autocomplete="off" />

			<div>
				{{#header.primaryNav}}
					<ul id="xrx_bnrv4_header_nav_primary">
						{{> ../includes/services}}
						{{> ../includes/products}}
						{{> ../includes/insights}}
						{{> ../includes/supplies}}
						{{> ../includes/support}}
						{{> ../includes/about}}
					</ul>
				{{/header.primaryNav}}

				{{#header.secondaryNav}}
					<ul id="xrx_bnrv4_header_nav_secondary">
						{{#links}}
							<li id="{{id}}">
								{{> ../includes/mustachePartials/secondary_nav_link}}
							</li>
						{{/links}}
					</ul>
				{{/header.secondaryNav}}

				{{#header.tertiaryNav}}
					<ul id="xrx_bnrv4_header_nav_tertiary_mobile">
						{{> ../includes/contact_mobile}}
						{{> ../includes/share_mobile}}
					</ul>
				{{/header.tertiaryNav}}
			</div>
		</div>
	</div>
{{/header}}
